{
  "projectName": "FormCoach",
  "structure": {
    "pages": "src/pages",
    "components": "src/components/ui",
    "sharedLogic": "src/lib",
    "assets": "public"
  },
  "rules": {
    "noAutogeneratedNav": true,
    "noThemes": true,
    "keepFolderNames": true,
    "preserveTailwind": true
  },
  "aiBehavior": {
    "preferExtendingExisting": true,
    "askBeforeCreatingNewFiles": true,
    "neverOverwrite": true
  }
}